<td class="sonata-ba-list-field sonata-ba-list-field-{{ field_description.type }}" objectId="{{ admin.id(object) }}">
    {%- block field %}
        {% if value %}
            <a href="{{ value }}" target="_blank">
                {% if field_description.options.title is defined %}
                    {{ field_description.options.title }}
                {% else %}
                    {{ value }}
                {% endif %}
            </a>
        {% else %}
            N/A
        {% endif %}
    {% endblock -%}
</td>
